citHeader("To cite schite in publications use:")

citEntry(
  entry    = "Manual",
  title    = "schite: Citations for shiny",
  author   = person(given="Ed", family="Baker"),
  year     = "2022",
  url      = "https://github.com/edwbaker/schite",
  textVersion = paste(
    "Baker, E. (2021) schite. https://github.com/edwbaker/schite."
  )
)
